<div class="w-full">
  <% if notice.present? %>
    <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
  <% end %>

  <div class="min-w-full flex flex-col justify-between items-start ">
    <% @support_chats.each do |support_chat| %>
      <%= render partial: support_chat, locals: { support_chat: support_chat }  %>
    <% end %>
  </div>
</div>
